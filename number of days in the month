month_no=int(input("enter the month number:"))
if month_no==2:
  print("Month has 28 or 29 days")
elif (month_no==1 or month_no==3 or month_no==5 or month_no==7 or month_no==8 or month_no==10 or month_no==12):
  print("Month has 31 days")
elif (month_no==4 or month_no==6 or month_no==9 or month_no==11):
  print("Month has 30 days")
else:
  print("Invalid Input")
